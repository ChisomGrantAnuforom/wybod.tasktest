<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <SpaRoot>ClientApp\</SpaRoot>
        <SpaProxyEnabled>true</SpaProxyEnabled>
        <SpaProxyServerUrl>http://localhost:5173</SpaProxyServerUrl>
        <SpaProxyLaunchCommand>npm run dev</SpaProxyLaunchCommand>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.AspNetCore.SpaProxy" Version="8.0.0" />
    </ItemGroup>

    <ItemGroup>
      <_ContentIncludedByDefault Remove="Wybod.UnitTests\obj\project.assets.json" />
      <_ContentIncludedByDefault Remove="Wybod.UnitTests\obj\project.packagespec.json" />
      <_ContentIncludedByDefault Remove="Wybod.UnitTests\obj\Wybod.UnitTests.csproj.nuget.dgspec.json" />
    </ItemGroup>

</Project>
